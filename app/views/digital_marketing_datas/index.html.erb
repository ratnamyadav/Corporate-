 

<h1>Digital Marketing Datas</h1>

<table class="table table-bordered table-responsive">
  <thead>
    <tr>
      <th>Company</th>
      <th>Date</th>
      <th>Amount</th>
      <th>Search engine</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @digital_marketing_datas.each do |digital_marketing_data| %>
      <tr>
        <td><%= digital_marketing_data.company.try(:name) %></td>
        <td><%= digital_marketing_data.date %></td>
        <td><%= digital_marketing_data.amount %></td>
        <td><%= digital_marketing_data.search_engine %></td>
        <td><%= link_to 'Show', digital_marketing_data_path(digital_marketing_data) %></td>
        <td><%= link_to 'Edit', edit_digital_marketing_data_path(digital_marketing_data) %></td>
        <td><%= link_to 'Destroy', digital_marketing_data_path(digital_marketing_data), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Digital Marketing Data', new_digital_marketing_data_path %>
