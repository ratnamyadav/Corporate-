<%= form_for(company_lease_information) do |f| %>
  <% if company_lease_information.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(company_lease_information.errors.count, "error") %> prohibited this company_lease_information from being saved:</h2>

      <ul>
      <% company_lease_information.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :company %>
    <%= f.select :company_id, options_from_collection_for_select(Company.all, :id, :name, company_lease_information.company_id), class: 'form-control' %>
    <br /><br />
  </div>

  <div class="field">
    <%= f.label :house %>
    <%= f.select :house_id, options_from_collection_for_select(House.all, :id, :address, company_lease_information.company_id), class: 'form-control' %>
    <br /><br />
  </div>

  <div class="field">
    <%= f.label :start_date %>
    <%= f.date_field :start_date, class: 'form-control' %>
    <br />
  </div>

  <div class="field">
    <%= f.label :end_date %>
    <%= f.date_field :end_date, class: 'form-control' %>
    <br />
  </div>

  <div class="field">
    <%= f.label :total_lease_amount %>
    <%= f.text_field :total_lease_amount, class: 'form-control' %>
    <br />
  </div>

  <div class="actions">
     <%= f.submit 'Submit', class: 'btn btn-success' %>
  </div>
<% end %>
