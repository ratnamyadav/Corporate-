 

<h1>Company Names</h1>

<table class="table table-bordered table-responsive">
  <thead>
    <tr>
      <th>Date</th>
      <th>Amount</th>
      <th>Payroll type</th>
      <th>Search engine</th>
      <th>Month</th>
      <th>Year</th>
      <th>Miscellaneous expense type</th>
      <th>Company</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @company_names.each do |company_name| %>
      <tr>
        <td><%= company_name.date %></td>
        <td><%= company_name.amount %></td>
        <td><%= company_name.payroll_type %></td>
        <td><%= company_name.search_engine %></td>
        <td><%= company_name.month %></td>
        <td><%= company_name.year %></td>
        <td><%= company_name.miscellaneous_expense_type %></td>
        <td><%= company_name.company_id %></td>
        <td><%= link_to 'Show', company_name %></td>
        <td><%= link_to 'Edit', edit_company_name_path(company_name) %></td>
        <td><%= link_to 'Destroy', company_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<hr /><%= link_to 'New Company Name', new_company_name_path %>
